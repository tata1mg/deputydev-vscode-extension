# === Example env file for VS Code Extension ===
# Copy this file to `.env` and update values as needed

# API host for deputydev backend
DD_HOST=https://api.deputydev.ai
# DD_HOST=http://localhost:8084
# BINARY_DD_HOST=http://deputydev-extension-backend:8084

# Whether to enable the VS Code output channel (true/false)
ENABLE_OUTPUT_CHANNEL=false

# Whether to skip downloading binary and use a locally running binary instead (true/false)
USE_LOCAL_BINARY=false

# Port used when USE_LOCAL_BINARY=true
LOCAL_BINARY_PORT=8001
